<aura:component access="global" implements="lightning:availableForFlowScreens" controller="SwipeyFlowController">

    <!-- Libraries --> 
    <ltng:require styles="{!join(',',   
                    $Resource.SwipeyJs + '/css/jTinder.css'
                    )}" scripts="{!join(',', 
                    $Resource.SwipeyJs + '/js/jquery321min.js',
                    $Resource.SwipeyJs + '/js/jquery.transform2d.js', 
                    $Resource.SwipeyJs + '/js/jquery.jTinder.js'
                )}" afterScriptsLoaded="{!c.handleInit}" />

    <!-- Attributes : Inputs -->
    <aura:attribute name="completedMessage" type="String" default="We have saved your preferences." />
    <aura:attribute name="imageCategory" type="String"/>
    <aura:attribute name="likeImageUrl" type="String"/>
    <aura:attribute name="dislikeImageUrl" type="String"/>



    <!-- Attributes : Internal --> 
    <aura:attribute name="staticResourceZipImagefiles" type="List" default="" />
    <aura:attribute name="likedItems" type="List" default="" />
    <aura:attribute name="dislikedItems" type="List" default="" />
    <aura:attribute name="sitePrefix" type="String" />
    <aura:attribute name="showSpinner" type="Boolean" default="true"/>
    <!-- Attributes : Outputs -->
    <aura:attribute name="multipicklistSwipeyResult" type="String" default="" />


    <!-- Component : Internal -->
    <div class="slds-m-top_medium">
        <aura:if isTrue="{!v.showSpinner}">
            <lightning:spinner alternativeText="Loading" size="medium" />
        </aura:if>
        <div id="tinderslide" class="slds-m-around_xx-small">
            <aura:if isTrue="{! v.staticResourceZipImagefiles.length > 0 }">
                <ul>
                    <aura:iteration items="{! v.staticResourceZipImagefiles }" var="item" indexVar="idx">
                        <li class="{!'pane' + (idx+1) }">
                            <div class="img"
                                style="{! 'background:url(' + '\'' + item.url + '\'' + ') no-repeat scroll center center !important; background-size: contain !important;'}">
                            </div>
                            <div class="imageName">{!item.title}</div>
                            <div class="imageDesc slds-text-body_small">{!item.desc}</div>
                            <div class="like" style="{# not(empty(v.likeImageUrl)) ? 'background:url(\'' + v.likeImageUrl + '\') no-repeat; background-size:contain;' : ''}"></div> 
                            <div class="dislike" style="{# not(empty(v.dislikeImageUrl)) ? 'background:url(\'' + v.dislikeImageUrl + '\') no-repeat; background-size:contain;' : ''}"></div>
                        </li>
                    </aura:iteration>
                </ul>
                <aura:set attribute="else">
                    <div id="errormessage" class="slds-text-align_center slds-hide">
                        <div class="slds-notify slds-notify_alert slds-theme_alert-texture slds-theme_warning" role="alert">
                            <span class="slds-assistive-text">error</span>
                            <p>{!v.imageCategory != null ? 'No records returned with the selected category' : 'No records found'}</p>
                        </div> 
                        <img src="data:image/png;base64," style="padding: 3rem;"/> 
                    </div> 
                </aura:set>
            </aura:if>
        </div>
        <div id="tinderslide_output" class="hide"
            style="background-image:url('data:image/png;base64,');">

        </div>

    </div>
</aura:component>